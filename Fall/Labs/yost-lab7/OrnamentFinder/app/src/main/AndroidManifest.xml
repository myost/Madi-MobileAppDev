<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.madi.ornamentfinder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_ornament"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_ornament_round"
        android:supportsRtl="true"
        android:theme="@style/OrnamentAppTheme">
        <activity android:name=".FindOrnamentActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecieveOrnamentActivity"></activity>
    </application>

</manifest>